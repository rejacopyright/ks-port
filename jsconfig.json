{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@pages/*": [
                "pages/*"
            ],
            "@components/*": [
                "_components/*"
            ],
            "@helpers/*": [
                "_helpers/*"
            ],
            "@api/*": [
                "_api/*"
            ],
            "@redux": [
                "_redux"
            ],
            "@redux/*": [
                "_redux/*"
            ],
            "@styles/*": [
                "styles/*"
            ],
            "@public/*": [
                "public/*"
            ],
            "@images/*": [
                "public/images/*"
            ]
        },
    },
    "exclude": [
        "node_modules"
    ]
}